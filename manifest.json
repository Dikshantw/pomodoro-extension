{
  "manifest_version": 3,
  "name": "Pomodoro Timer",
  "description": "Base Level Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons8-timer-16.png",
      "32": "icons8-timer-32.png"
    }
  },
  "permissions": ["storage", "background"]
}
